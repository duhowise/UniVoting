    <ControlTemplate x:Key="ScrollBarThumbGripHorizontal" TargetType="ContentControl">
        <Grid>
            <Rectangle Fill="{TemplateBinding Background}" Width="6" Height="9"/>
            <Path Stroke="{TemplateBinding Foreground}" Data="M0.5,0 L0.5,9 M2.5,0 L2.5,9 M4.5,0 L4.5,9" Width="6" Height="9"/>
        </Grid>
    </ControlTemplate>
