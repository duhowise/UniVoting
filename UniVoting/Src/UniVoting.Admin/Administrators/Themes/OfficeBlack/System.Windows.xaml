            <Rectangle Fill="{TemplateBinding Background}" Width="9" Height="6"/>
            <Path Stroke="{TemplateBinding Foreground}" Data="M0,0.5 L9,0.5 M0,2.5 L9,2.5 M0,4.5 L9,4.5" Width="9" Height="6"/>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="ScrollArrow_Up" TargetType="ContentControl">
