            <Path x:Name="ForegroundIcon" Data="M0,0 L2,0 1,1 Z" Stretch="Fill" Width="7" Height="4" Fill="{TemplateBinding Foreground}" Margin="0 1 0 1"/>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="ScrollArrow_Left" TargetType="ContentControl">
