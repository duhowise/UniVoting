<Window x:Class="UniVoting.Admin.Administrators.ReportViewerWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
    Title="Report Viewer Window">

    <Grid x:Name="LayoutRoot">
        <Grid Margin="20" >
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <WindowsFormsHost Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                <rv:ReportViewer x:Name="ReportViewer1" />
            </WindowsFormsHost>

        </Grid>
    </Grid>
</Window>
